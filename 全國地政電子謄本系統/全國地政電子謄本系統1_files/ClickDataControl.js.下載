function IDR_Click() {

    /*****************************************
    ☆當點選異動清冊時的顯示控制
    *****************************************/

    //個資----------------------------------------------------------------------
    for (var i = 0; i < ApplyForm.INPUT_0731_A.length; i++) {
        ApplyForm.INPUT_0731_A[i].disabled = !ApplyForm.INPUT_071.checked;
        ApplyForm.INPUT_0731_A[i].checked = false;
    }

    for (var i = 0; i < ApplyForm.INPUT_075.length; i++) {
        ApplyForm.INPUT_075[i].disabled = !ApplyForm.INPUT_071.checked;
        ApplyForm.INPUT_075[i].checked = false;
    }


    for (var i = 0; i < ApplyForm.INPUT_076.length; i++) {
        ApplyForm.INPUT_076[i].disabled = !ApplyForm.INPUT_075[1].checked;
        ApplyForm.INPUT_076[i].checked = false;
    }

//    ApplyForm.INPUT_077.disabled = !ApplyForm.INPUT_075[0].checked; //統編
//    ApplyForm.INPUT_078.disabled = !ApplyForm.INPUT_075[1].checked; //登記次序
//    ApplyForm.INPUT_079.disabled = !ApplyForm.INPUT_075[2].checked; //姓名
    ApplyForm.INPUT_075[0].checked = ApplyForm.INPUT_071.checked; //勾選異動清冊後預設為「完整」
    ApplyForm.INPUT_0731_A[0].checked = ApplyForm.INPUT_071.checked; //勾選異動清冊後預設為「本縣市」
    ChangeINPUT_0731_A();

    //------------------------------------------------------------------------個資


    //預設勾選地號+統編選項   
    ApplyForm.INPUT_0731.disabled = !ApplyForm.INPUT_071.checked;
    ApplyForm.INPUT_0732.disabled = !ApplyForm.INPUT_071.checked;
    ApplyForm.INPUT_07322.disabled = !ApplyForm.INPUT_071.checked;
    ApplyForm.INPUT_0733.disabled = !ApplyForm.INPUT_071.checked;
    ApplyForm.INPUT_0734.disabled = !ApplyForm.INPUT_071.checked;
    ApplyForm.INPUT_07342.disabled = !ApplyForm.INPUT_071.checked;
    ApplyForm.IDR_CNT.disabled=!ApplyForm.INPUT_071.checked;
    var userapplytype = ApplyForm.UserApplyType.value;

    if (ApplyForm.INPUT_071.checked) {
        ApplyForm.INPUT_0731.className = "";
        ApplyForm.INPUT_0732.selectedIndex = 0;
        ApplyForm.INPUT_07321.selectedIndex = 0;
        ApplyForm.INPUT_07322.selectedIndex = 0;
        ApplyForm.INPUT_0733.className = "";
        ApplyForm.INPUT_0731.focus();
        ApplyForm.INPUT_0734.className = "";
        ApplyForm.INPUT_0734.value = ApplyForm.INPUT_0732.options[ApplyForm.INPUT_0732.selectedIndex].value;
        ApplyForm.INPUT_07342.className = "";
        ApplyForm.INPUT_07342.value = ApplyForm.INPUT_07322.options[ApplyForm.INPUT_07322.selectedIndex].value;
 
    ApplyForm.IDR_CNT.className="";
    ApplyForm.IDR_CNT.value = "1";
    if ( CertType == "Y") 
    {//憑証登入
		if (userapplytype == "1" )
		{
			//申請人身份為「登記名義人本人」,須帶入統編,不可更改 2014-02-11
		    ApplyForm.INPUT_015.value = CertID;
		    ApplyForm.INPUT_015.disabled = true;
		} else if (userapplytype == "2") {
		    //申請人身份為「登記名義人代理人」,不要帶憑證上的統編，可修改 2014-02-11
		    //2014-08-04 申請人身份為「登記名義人代理人」,申請異動清冊時,要帶憑證上的統編
		    //ApplyForm.INPUT_015.value = CertID;
		}
	 }	
    } else {
        ApplyForm.INPUT_0731.className = "inputtext_disabled";
        ApplyForm.INPUT_0732.value = "";
        ApplyForm.INPUT_07321.value = "";
        ApplyForm.INPUT_07322.value = "";
        ApplyForm.INPUT_0733.className = "inputtext_disabled";
        ApplyForm.INPUT_0734.value = "";
        ApplyForm.INPUT_0734.className = "inputtext_disabled";
        ApplyForm.INPUT_07342.value = "";
        ApplyForm.INPUT_07342.className = "inputtext_disabled";
        ApplyForm.INPUT_077.className = "inputtext_disabled";
        ApplyForm.INPUT_077.value = "";
        ApplyForm.INPUT_078.className = "inputtext_disabled";
        ApplyForm.INPUT_078.value = "";
        ApplyForm.INPUT_079.className = "inputtext_disabled";
        ApplyForm.INPUT_079.value = "";
        //ApplyForm.IDR_CNT.value="";
        ApplyForm.IDR_CNT.className = "inputtext_disabled";
    if (CertType == "Y") 
    {//憑証登入
        //if (userapplytype == "1" || userapplytype == "2")
        //{
        if (userapplytype == "1") {//申請人身份為「登記名義人本人」
                ApplyForm.INPUT_015.value = CertID;
                ApplyForm.INPUT_015.disabled = true;
            } else {
                //2014-08-04 申請人身份為「登記名義人代理人」,申請異動清冊時,要帶憑證上的統編
                //ApplyForm.INPUT_015.value = CertID;
                ApplyForm.INPUT_015.disabled = false;

            }
        //}
    }
  }   
    ApplyForm.INPUT_0731.value = "";
    ApplyForm.INPUT_0733.value = "";


}


function IRSTYLE_Click() {
    /*****************************************
    ☆當點選異動清冊選項時的顯示控制
    *****************************************/

    //勾選異動清冊為「部份」
    for (var i = 0; i < ApplyForm.INPUT_076.length; i++) {
        ApplyForm.INPUT_076[i].disabled = !ApplyForm.INPUT_075[1].checked;
        ApplyForm.INPUT_076[i].checked = false;
    }
    ApplyForm.INPUT_076[0].checked = ApplyForm.INPUT_075[1].checked; //選「部份」後預設為「統一编號」
    ApplyForm.INPUT_077.disabled = !ApplyForm.INPUT_075[1].checked; //統編

    if (ApplyForm.INPUT_075[1].checked) {
        ApplyForm.INPUT_077.className = "";
        ApplyForm.INPUT_077.value = "";
    } else {
        ApplyForm.INPUT_077.disabled = !ApplyForm.INPUT_075[1].checked; //統編
        ApplyForm.INPUT_078.disabled = !ApplyForm.INPUT_075[1].checked; //
        ApplyForm.INPUT_079.disabled = !ApplyForm.INPUT_075[1].checked; //
        ApplyForm.INPUT_077.className = "inputtext_disabled";
        ApplyForm.INPUT_077.value = "";
        ApplyForm.INPUT_078.className = "inputtext_disabled";
        ApplyForm.INPUT_078.value = "";
        ApplyForm.INPUT_079.className = "inputtext_disabled";
        ApplyForm.INPUT_079.value = "";
    }

    ApplyForm.INPUT_0731.focus();

}


function IRP_Click() {
    /*****************************************
    ☆當點選異動清冊的「部份」選項時的顯示控制
    *****************************************/
    ApplyForm.INPUT_077.disabled = !ApplyForm.INPUT_076[0].checked;
    ApplyForm.INPUT_078.disabled = !ApplyForm.INPUT_076[1].checked;
    ApplyForm.INPUT_079.disabled = !ApplyForm.INPUT_076[2].checked;
    if (ApplyForm.INPUT_076[0].checked) {
        ApplyForm.INPUT_077.className = "";
        ApplyForm.INPUT_077.value = "";
        ApplyForm.INPUT_078.className = "inputtext_disabled";
        ApplyForm.INPUT_078.value = "";
        ApplyForm.INPUT_079.className = "inputtext_disabled";
        ApplyForm.INPUT_079.value = "";
        ApplyForm.INPUT_077.focus();
    } else if (ApplyForm.INPUT_076[1].checked) {
        ApplyForm.INPUT_077.className = "inputtext_disabled";
        ApplyForm.INPUT_077.value = "";
        ApplyForm.INPUT_078.className = "";
        ApplyForm.INPUT_078.value = "";
        ApplyForm.INPUT_079.className = "inputtext_disabled";
        ApplyForm.INPUT_079.value = "";
        ApplyForm.INPUT_078.focus();
    } else {
        ApplyForm.INPUT_077.className = "inputtext_disabled";
        ApplyForm.INPUT_077.value = "";
        ApplyForm.INPUT_078.className = "inputtext_disabled";
        ApplyForm.INPUT_078.value = "";
        ApplyForm.INPUT_079.className = "";
        ApplyForm.INPUT_079.value = "";
        ApplyForm.INPUT_079.focus();
    }

}

function IS05_Click(){
	
  /*****************************************
  ☆當點選異動索引日期範圍時的顯示控制
  *****************************************/
  
    //預設不勾選
    ApplyForm.INPUT_0631.disabled = (!ApplyForm.INPUT_063.checked);
    ApplyForm.INPUT_0631Y.disabled = (!ApplyForm.INPUT_063.checked);
    ApplyForm.INPUT_0631M.disabled = (!ApplyForm.INPUT_063.checked);
    ApplyForm.INPUT_0631D.disabled = (!ApplyForm.INPUT_063.checked);
    ApplyForm.INPUT_0632.disabled = (!ApplyForm.INPUT_063.checked);
    ApplyForm.INPUT_0632Y.disabled = (!ApplyForm.INPUT_063.checked);
    ApplyForm.INPUT_0632M.disabled = (!ApplyForm.INPUT_063.checked);
    ApplyForm.INPUT_0632D.disabled = (!ApplyForm.INPUT_063.checked);

    if (ApplyForm.INPUT_063.checked)
    {
        ApplyForm.INPUT_0631.className = "";
        ApplyForm.INPUT_0631Y.className = "";
        ApplyForm.INPUT_0631M.className = "";
        ApplyForm.INPUT_0631D.className = "";
        ApplyForm.INPUT_0632.className = "";
        ApplyForm.INPUT_0632Y.className = "";
        ApplyForm.INPUT_0632M.className = "";
        ApplyForm.INPUT_0632D.className = "";
        ApplyForm.INPUT_0631.focus();
    } else
    {
        ApplyForm.INPUT_0631.className = "inputtext_disabled";
        ApplyForm.INPUT_0631Y.className = "inputtext_disabled";
        ApplyForm.INPUT_0631M.className = "inputtext_disabled";
        ApplyForm.INPUT_0631D.className = "inputtext_disabled";
        ApplyForm.INPUT_0632.className = "inputtext_disabled";
        ApplyForm.INPUT_0632Y.className = "inputtext_disabled";
        ApplyForm.INPUT_0632M.className = "inputtext_disabled";
        ApplyForm.INPUT_0632D.className = "inputtext_disabled";
    }

    ApplyForm.INPUT_0631.value = "";
    ApplyForm.INPUT_0631Y.value = "";
    ApplyForm.INPUT_0631M.value = "";
    ApplyForm.INPUT_0631D.value = "";
    ApplyForm.INPUT_0632.value = "";
    ApplyForm.INPUT_0632Y.value = "";
    ApplyForm.INPUT_0632M.value = "";
    ApplyForm.INPUT_0632D.value = "";

}

